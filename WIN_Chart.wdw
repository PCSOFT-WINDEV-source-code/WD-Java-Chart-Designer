            -
              internal_properties : 
              code : |1-
               sChartName is string = "MyScatterChart"
               
               // Create the chart
               grCreate(sChartName,grScatter) 
               
               // Configure the destination of the chart
               grDestinationControl(sChartName,"WIN_Chart.IMG_Scatter")
               
               // Do we have to link the dots?
               grScatterLinkPoint(sChartName,CBOX_Link)
               
               // Define the diameter of the dot
               grScatterPointSize(sChartName,EDT_DiameterDots)
               
               // Font of the chart
               grTitleFont(sChartName,"Arial",10,Black)
               
               SWITCH RADIO_TitlePosition
               	CASE 1 // At the top
               		
               		// Title of the chart
               		grTitle(sChartName,<§$0006§>,grAtTop)
               		
               	CASE 2 // At the bottom
               		
               		// Title of the chart
               		grTitle(sChartName,<§$0006§>,grAtBottom)
               		
               	CASE 3 // No title
               		// Title of the chart
               		grTitle(sChartName,<§$0006§>,grNone)		
               END
               
               SWITCH RADIO_LegendPosition
               	CASE 1 // on the left
               		// Position of the legend
               		grLegend(sChartName,grAtLeft)
               		
               	CASE 2 // to the right
               		
               		// Position of the legend
               		grLegend(sChartName,grAtRight)
               		
               	CASE 3 // Without legend
               		
               		// Position of the legend
               		grLegend(sChartName,grNone)	
               END
               
               
               LOCAL
               	i is int
               	
               FOR i = 1 TO 10
               	
               	// Add a data
               	grAddData(sChartName,1,Random(-150,150))
               	grAddData(sChartName,2,Random(-150,200))
               	grAddData(sChartName,3,Random(1,250))
               	
               	// Legend of this value
               	grCategoryLabel(sChartName,i,StringBuild(<§$0005§>,i))
               	
               END
               
               // Draw this chart
               grDraw(sChartName)
