          name : BTN_DrawHistogram
          identifier : 0x286fcf940680cd94
          internal_properties : 
          properties :
           x : 437
           y : 439
           width : 95
           height : 26
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              internal_properties : 
              code : |1-
               sChartName is string = <§$0000§>
               
               // Create the chart
               grCreate(sChartName,grColumn)
               
               // Configure the destination of the chart
               grDestinationControl(sChartName,"WIN_Chart.IMG_Column_chart")
               
               // Font of the chart
               grTitleFont(sChartName,"Arial",10,Black)
               
               // Title of the chart
               SWITCH RADIO_TitlePosition
               	CASE 1 // At the top
               		grTitle(sChartName,<§$0001§>,grAtTop)
               		
               	CASE 2 // At the bottom
               		grTitle(sChartName,<§$0001§>,grAtBottom)
               		
               	CASE 3 // No title
               		grTitle(sChartName,<§$0001§>,grNone)		
               END
               
               // Position of the legend
               SWITCH RADIO_LegendPosition
               	CASE 1 // on the left
               		grLegend(sChartName,grAtLeft)
               		
               	CASE 2 // to the right
               		grLegend(sChartName,grAtRight)
               		
               	CASE 3 // Without legend
               		grLegend(sChartName,grNone)	
               END
               
               
               LOCAL
               	i is int
               	
               FOR i = 1 TO 20
               	
               	// Add a data
               	grAddData(sChartName,1,Random(0,200))
               	
               	// Legend of this value
               	grCategoryLabel(sChartName,i,StringBuild(<§$0002§>,i))
               	
               END
               
               // Draw this chart
               grDraw(sChartName)
